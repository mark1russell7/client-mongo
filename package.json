{
  "$schema": "https://json.schemastore.org/package",
  "name": "@mark1russell7/client-mongo",
  "version": "1.0.0",
  "description": "MongoDB client wrapper with client procedures - local or RPC access",
  "license": "MIT",
  "author": "Mark Russell <marktheprogrammer17@gmail.com>",
  "devDependencies": {
    "@mark1russell7/cue": "github:mark1russell7/cue#main",
    "@types/node": "^22.0.0",
    "typescript": "^5.9.3"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./procedures": {
      "types": "./dist/procedures/index.d.ts",
      "import": "./dist/procedures/index.js"
    },
    "./register": {
      "types": "./dist/register.d.ts",
      "import": "./dist/register.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -b",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .tsbuildinfo",
    "prepare": "npm run build",
    "postinstall": "client announce"
  },
  "sideEffects": false,
  "client": {
    "procedures": "./dist/register.js"
  },
  "keywords": [
    "mongodb",
    "client",
    "procedures",
    "rpc",
    "database"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mark1russell7/client-mongo.git"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "mongodb": "^7.0.0",
    "@mark1russell7/client": "github:mark1russell7/client#main"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@mark1russell7/cue",
      "@mark1russell7/client"
    ]
  }
}
